
public class App {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String[] imena = new String[5];
		
		imena[0]="Predrag N";
		imena[1]="Srdjan D";
		imena[2]="Mladen T";
		imena[3]="Milan L";
		imena[4]="Marko M";
		
		for (String ime:imena) {
			
			System.out.println(ime);
		}

	}

}
